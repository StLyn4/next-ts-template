{
  "name": "next-ts-template",
  "version": "0.1.0",
  "description": "Шаблон Next.js с TypeScript, блэкджеком и хорошими вещами",
  "author": "Vsevolod Volkov <st.lyn4@gmail.com> (https://github.com/StLyn4)",
  "repository": "github:StLyn4/next-ts-template",
  "homepage": "https://github.com/StLyn4/next-ts-template",
  "bugs": "https://github.com/StLyn4/next-ts-template/issues",
  "license": "Apache-2.0",
  "private": true,
  "keywords": [
    "react",
    "next",
    "typescript",
    "template"
  ],
  "scripts": {
    "postinstall": "next telemetry disable | dev-null & husky install",
    "postbuild": "next-sitemap",
    "dev": "next dev",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "docs:dev": "start-storybook",
    "docs:build": "build-storybook",
    "analyze": "cross-env ANALYZE_BUNDLE=true next build --no-lint",
    "test": "chromatic",
    "lint": "npm-run-all --parallel --continue-on-error lint:*",
    "lint:code": "next lint --fix --dir . & tsc --noEmit",
    "lint:configs": "prettier --loglevel warn --write {**/*,*}.{json,yml,yaml}",
    "lint:styles": "stylelint --allow-empty-input --fix {**/*,*}.{css,scss,sass}",
    "lint:markdown": "prettier --loglevel warn --write {**/*,*}.{md,mdx} && markdownlint --fix {**/*,*}.{md,mdx}",
    "lint:spelling": "cspell --show-context --no-progress --no-must-find-files **/*",
    "snyk:test": "dotenv -c -- snyk test",
    "release": "release-it --ci",
    "clean": "rimraf --glob .next out dist build temp .tmp tmp node_modules/.cache {**/*,*}.tsbuildinfo"
  },
  "dependencies": {
    "@floating-ui/react": "^0.24.7",
    "@fontsource/inter": "^5.0.5",
    "@mantine/hooks": "^6.0.17",
    "@next/bundle-analyzer": "^13.4.10",
    "@next/env": "^13.4.10",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.1",
    "classnames": "^2.3.2",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^10.13.0",
    "maplibre-gl": "^3.2.0",
    "maplibregl-mapbox-request-transformer": "^0.0.2",
    "modern-normalize": "^2.0.0",
    "next": "13.4.10",
    "next-translate": "^2.5.0",
    "next-translate-plugin": "^2.5.0",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-calendar": "^4.4.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "^2.9.5",
    "react-inlinesvg": "^3.0.2",
    "react-map-gl": "^7.1.2",
    "react-phone-number-input": "^3.3.0",
    "react-redux": "^8.1.1",
    "react-textarea-autosize": "^8.5.2",
    "react-virtuoso": "^4.4.1",
    "react18-input-otp": "^1.1.4",
    "sharp": "^0.32.3",
    "use-custom-compare": "^1.4.0",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@cspell/dict-ru_ru": "^2.1.0",
    "@types/mapbox-gl": "^2.7.12",
    "@types/node": "20.4.2",
    "@types/qs": "^6.9.7",
    "@types/react": "18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@typescript-eslint/utils": "^6.1.0",
    "chromatic": "^6.19.9",
    "cross-env": "^7.0.3",
    "cspell": "^6.31.1",
    "dev-null-cli": "^2.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.45.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^13.4.10",
    "eslint-plugin-jsdoc": "^46.4.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.2",
    "lint-staged": "^13.2.3",
    "markdownlint-cli": "^0.35.0",
    "next-sitemap": "^4.1.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.26",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^9.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^3.0.0",
    "release-it": "^16.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "snyk": "^1.1192.0",
    "stylelint": "<15.0.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recommended-scss": "<9.0.0",
    "ts-essentials": "^9.3.2",
    "typescript": "5.1.6"
  }
}
