{
  "name": "next-ts-template",
  "version": "1.0.0",
  "description": "Шаблон Next.js с TypeScript, блэкджеком и хорошими вещами",
  "author": "Vsevolod Volkov <st.lyn4@gmail.com> (https://github.com/StLyn4)",
  "repository": "github:StLyn4/next-ts-template",
  "homepage": "https://github.com/StLyn4/next-ts-template",
  "bugs": "https://github.com/StLyn4/next-ts-template/issues",
  "license": "Apache-2.0",
  "private": true,
  "keywords": [
    "react",
    "next",
    "template",
    "typescript"
  ],
  "scripts": {
    "postinstall": "next telemetry disable | dev-null & husky install",
    "postbuild": "next-sitemap --config sitemap.config.js",
    "dev": "next dev",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "docs:dev": "start-storybook",
    "docs:build": "build-storybook",
    "analyze": "cross-env ANALYZE=true next build --no-lint",
    "test": "chromatic",
    "lint": "npm-run-all --parallel --continue-on-error lint:*",
    "lint:code": "next lint --fix",
    "lint:configs": "prettier --loglevel warn --write {**/*,*}.{json,yml,yaml}",
    "lint:styles": "stylelint --fix {**/*,*}.{css,scss,sass}",
    "lint:markdown": "prettier --loglevel warn --write {**/*,*}.{md,mdx} && markdownlint --fix {**/*,*}.{md,mdx}",
    "lint:spelling": "cspell --show-context --no-progress --no-must-find-files **/*",
    "release": "release-it --ci"
  },
  "dependencies": {
    "@mantine/hooks": "^5.10.2",
    "@next/env": "^13.0.5",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.0",
    "axios-retry": "^3.2.5",
    "classnames": "^2.3.2",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^8.5.2",
    "next": "13.1.5",
    "next-translate": "npm:@lyn4ed/next-translate@^2.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-inlinesvg": "^3.0.1",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^8.0.5",
    "sharp": "^0.31.2",
    "use-debounce": "^9.0.2"
  },
  "devDependencies": {
    "@cspell/dict-ru_ru": "^2.0.4",
    "@next/bundle-analyzer": "^13.0.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "chromatic": "^6.11.4",
    "cross-env": "^7.0.3",
    "cspell": "^6.15.1",
    "dev-null-cli": "^2.0.0",
    "eslint": "^8.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^13.0.5",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "markdownlint-cli": "^0.33.0",
    "next-sitemap": "^3.1.32",
    "next-translate-plugin": "^2.0.0-canary.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.0",
    "release-it": "^15.6.0",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "ts-essentials": "^9.3.0",
    "typescript": "4.9.4"
  }
}
